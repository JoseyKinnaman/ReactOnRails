<script type="text/javascript">
  $(function() {
    return $(".sortable").sortable({
      axis: "y",
      handle: ".handle",
      update: function() {
        return $.post($(this).data("update-url"), $(this).sortable("serialize"));
      }
    });
  });
</script>
